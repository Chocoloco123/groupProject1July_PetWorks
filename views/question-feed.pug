div(id='questionFeed')
    each question in questions
        div(class='question')
            a(href=`/questions/${question.id}`)= question.question
        if question.userId === userId  
            div 
                a(href=`/questions/${question.id}/edit`) Edit 
                a(id=`question-delete-${question.id}` class=('delete-button') href=``) Delete
                
            

